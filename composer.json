{
    "name": "imjoehaines/flowder-yaml-loader",
    "description": "A YAML file loader for flowder",
    "keywords": [
        "fixtures",
        "fixture-loading",
        "testing",
        "yaml"
    ],
    "homepage": "https://github.com/imjoehaines/flowder-yaml-loader",
    "license": "Unlicense",
    "authors": [
        {
            "name": "Joe Haines",
            "email": "hello@joehaines.co.uk",
            "homepage": "https://www.joehaines.co.uk",
            "role": "Developer"
        }
    ],
    "scripts": {
        "test": "phpunit tests/"
    },
    "config": {
        "platform": {
            "php": "5.6"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Imjoehaines\\FlowderYaml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imjoehaines\\FlowderYaml\\Test\\": "tests/"
        }
    },
    "require": {
        "imjoehaines/flowder": "dev-master",
        "symfony/yaml": "^3.2"
    }
}
